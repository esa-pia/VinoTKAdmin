<h1>Listing millesimes</h1>

<table>
  <tr>
    <th>Valeur</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @millesimes.each do |millesime| %>
  <tr>
    <td><%= millesime.valeur %></td>
    <td><%= link_to 'Show', millesime %></td>
    <td><%= link_to 'Edit', edit_millesime_path(millesime) %></td>
    <td><%= link_to 'Destroy', millesime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Millesime', new_millesime_path %>
